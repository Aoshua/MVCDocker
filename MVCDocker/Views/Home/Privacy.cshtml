@model List<MVCDocker.Models.Planets> 
@{
    ViewData["Title"] = "Our Solar System";
}
<h1>@ViewData["Title"]</h1>



<table class="table">
    <thead>
        <tr>
            <th scope="col">Number from Sun</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var planet in Model)
        {
            <tr>
                <td>@planet.Id</td>
                <td>@planet.Name</td>
            </tr>
        }
    </tbody>
</table>
